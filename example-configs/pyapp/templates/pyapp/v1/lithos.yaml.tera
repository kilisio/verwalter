# Generated by verwalter {{ verwalter_version }}

{% for key, daemon in daemons %}
{{ key }}:
    kind: Daemon
    image: {{ daemon.image }}
    config: {{ daemon.config }}
    instances: {{ daemon.instances }}
{% endfor %}

{% if commands is defined %}
{% for key, command in commands %}
{{ key }}:
    kind: Command
    image: {{ command.image }}
    config: {{ command.config }}

{% endfor %}
{% endif %}
